@{
}
@model IEnumerable<string>
<a asp-controller="Home" asp-action="Index" class="btn @(ViewBag.SelectedType == null ? "btn-primary" : "btn-outline-secondary") col-md">Home</a>

@foreach (string t in Model)
{
    <a asp-controller="Home" asp-action="Index" asp-route-teamName="@t"
        class="btn @(ViewBag.SelectedType == t ? "btn-primary" : "btn-outline-secondary") col-md">@t</a>

}